pipeline {
    agent {
        docker { image 'blazemeter/taurus' }
    }
    stages {
        stage('install bzt') {
            steps {
                sh 'pip install bzt'
            }
        }
        stage('test perf') {
            steps {
                sh 'bzt "fatboar-api/test/perf.jmx"'
            }
        } 
    }
} 
